{
	"nodeModulesDir": "auto",
	"tasks": {
		"dev": "deno run -A --watch npm:vike dev",
		"build": "deno run -A npm:vike build",
		"preview": "deno run -A npm:vike preview",
		"lint": "biome lint --write",
		"format": "biome format --write",
		"check": "biome check --write"
	},
	"compilerOptions": {
		"types": [
			"react",
			"react-dom",
			"@types/react",
			"vite/client",
			"vike-cloudflare/types",
			"vike-react"
		],
		"lib": [
			"dom",
			"dom.iterable",
			"esnext",
			"deno.ns"
		],
		"jsx": "react-jsx",
		"jsxImportSource": "react"
	},
	"lint": {
		"exclude": [
			"./deno.lock",
			"./dist",
			"./node_modules"
		]
	},
	"imports": {
		"@biomejs/biome": "npm:@biomejs/biome@^2.3.11",
		"@radix-ui/react-accordion": "npm:@radix-ui/react-accordion@1.2.12",
		"@styled-icons/ionicons-solid": "npm:@styled-icons/ionicons-solid@^10.46.0",
		"@styled-icons/styled-icon": "npm:@styled-icons/styled-icon@^10.7.1",
		"@tailwindcss/postcss": "npm:@tailwindcss/postcss@4.1.14",
		"@tailwindcss/vite": "npm:@tailwindcss/vite@4.1.14",
		"clsx": "npm:clsx@2.1.1",
		"esbuild": "npm:esbuild@^0.25.10",
		"node-stdlib-browser": "npm:node-stdlib-browser@^1.3.1",
		"react": "npm:react@19.2.0",
		"react-dom": "npm:react-dom@19.2.0",
		"styled-components": "npm:styled-components@^6.3.8",
		"tailwind-merge": "npm:tailwind-merge@3.3.1",
		"typescript": "npm:typescript@^5.9.3",
		"vike": "npm:vike@^0.4.241",
		"vike-react": "npm:vike-react@^0.6.7",
		"vite-plugin-node-polyfills": "npm:vite-plugin-node-polyfills@^0.24.0",
		"@swc/core": "npm:@swc/core@1.13.5",
		"@tailwindcss/typography": "npm:@tailwindcss/typography@0.5.19",
		"@types/node": "npm:@types/node@24.6.2",
		"@types/react": "npm:@types/react@19.2.0",
		"@types/react-dom": "npm:@types/react-dom@19.2.0",
		"@vitejs/plugin-react-swc": "npm:@vitejs/plugin-react-swc@4.1.0",
		"autoprefixer": "npm:autoprefixer@10.4.21",
		"postcss": "npm:postcss@8.5.6",
		"tailwindcss": "npm:tailwindcss@4.1.14",
		"vite": "npm:vite@7.1.9",
		"@/": "./src/"
	}
}